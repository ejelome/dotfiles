! Lenovo ThinkPad Compact with TrackPoint

! Clear modifier keys
clear lock
clear control
clear mod1
clear mod4

! Left Ctrl
keycode  37 = Super_L

! Left âŠž Win
keycode 133 = Alt_L

! Left Alt
keycode  64 = Control_L

! Right Alt
keycode 108 = Control_R

! Right PrtSc
keycode 107 = Alt_R Meta_R

! Right Ctrl
keycode 105 = Print Sys_Req

! CapsLock
keycode  66 = Escape

! Esc
keycode   9 = Caps_Lock

! Re-add modifier keys
add lock    = Caps_Lock
add control = Control_L Control_R
add mod1    = Alt_L Alt_R Meta_L
add mod4    = Super_L Super_R Super_L Hyper_L
